
package {{go_package}}

{% if go_imports|length > 0 or go_private_imports|length > 0 %}
import (
{% for pkg in go_imports %}
	"{{pkg}}"
{% endfor %}{# pkg in go_imports #}
{% for pkg in go_private_imports %}
	"{{pkg}}"
{% endfor %}{# pkg in go_private_imports #}
)
{% endif %}{# len(go_imports) > 0 or len(go_private_imports) > 0 #}

{%- set interfacename = name | capfirst + "Interface" %}

type {{interfacename}} interface {
{% for m in methods %}
{% if m.is_request %}
{% if not m.async %} 
	{{m.name}}(remote RemoteNubInfo, arg *{{go_proto_package}}.{{m.arg}}, res *{{go_proto_package}}.{{m.res}})
{% else %}
	// You need to call reply_{{m.name.lower()}}(remote, replyid, res) after finished.
	{{m.name}}(remote RemoteNubInfo, replyid nfrpc.ReplyID, arg *{{go_proto_package}}.{{m.arg}})
{% endif %}{# not m.async #}
{% else %}
	{{m.name}}(remote RemoteNubInfo, arg *{{go_proto_package}}.{{m.arg}})
{% endif %}{# m.is_request #}
{% endfor %}{# m in methods #}

	handle_{{name.lower()}}_unknown_bubble(remote RemoteNubInfo, msgid nfrpc.MethodID, data *binary)
	handle_{{name.lower()}}_bad_bubble(remote RemoteNubInfo, msgid nfrpc.MethodID, data *binary)
};
